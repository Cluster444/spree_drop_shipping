<%= render :partial => 'admin/shared/supplier_tabs', :locals => {:current => "Supplier Details"} %>

<% if @supplier.try(:errors).present? %>
  <%= render 'shared/error_messages', :target => @supplier %>
<% end %>

<% form_for([:admin, @supplier]) do |f| %>
  <%= render "form", :form => f %>
  <p class="form-buttons">
    <%= button t("update") %> <%= t('or') %> <%= link_to t('cancel'), collection_url %>
  </p>
<% end %>